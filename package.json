        "lodash": "^4.17.19",
        "resolve-url-loader": "^3.1.0",
        "vue-axios": "^3.2.0",
        "vue-loader": "^15.9.5",
        "vue-router": "^3.4.9",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "@chenfengyuan/vue-carousel": "^1.0.5",
        "chart.js": "^2.9.4",
        "chartjs-plugin-datalabels": "^1.0.0",
        "ckeditor4-vue": "^1.3.1",
